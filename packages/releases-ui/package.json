{
  "name": "@bscotch/gamemaker-releases-ui",
  "version": "0.1.0",
  "description": "An unofficial Svelte UI for displaying GameMaker release change logs.",
  "keywords": [
    "gamemaker",
    "game-maker",
    "release",
    "release-notes",
    "patchnotes",
    "patch-notes",
    "changelog",
    "yoyo",
    "bscotch",
    "stitch",
    "rss",
    "feed",
    "ui",
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bscotch/gamemaker-info.git",
    "directory": "packages/releases"
  },
  "license": "MIT",
  "author": "Butterscotch Shenanigans",
  "type": "module",
  "scripts": {
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit sync && vite dev",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1"
  },
  "dependencies": {
    "@bscotch/gamemaker-releases": "workspace:*",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "svelte": "^3.53.1",
    "svelte-preprocess": "^4.10.7",
    "vite": "^3.2.5"
  },
  "devDependencies": {
    "@sveltejs/package": "next",
    "svelte-check": "^2.10.1",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
